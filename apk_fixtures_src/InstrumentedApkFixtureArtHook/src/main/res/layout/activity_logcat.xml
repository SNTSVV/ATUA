<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent">

	<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="button_2_logback_msgs"
					android:id="@+id/button_2_logback_msgs"
					android:onClick="button1Clicked"
	/>

	<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="button_3_other_msgs"
					android:id="@+id/button_3_other_msgs"
					android:onClick="button2Clicked"/>

	<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="button_junk_msgs"
					android:id="@+id/button_junk_msgs"
					android:onClick="junkButtonClicked"/>
</LinearLayout>

